method incr(this: Ref, tid: Int) returns (res: Int)
  requires this != null
  requires acc(this.x, 1 * write)
  ensures acc(this.x, 1 * write)
  ensures this.x == old(this.x) + 1
{
  var exc: Ref
  var return: Int
  var flatten: Ref
  var flatten1: Int
  exc := null
  flatten := this
  flatten1 := this.x + 1
  flatten.x := flatten1
  res := return
  assert exc == null
}

