method incr2(this: Ref, tid: Int, n: Int) returns (res: Int)
  requires this != null
  requires acc(this.count, 1 * write)
  ensures acc(this.count, 1 * write / 2)
  ensures this.count == old(this.count) + 2 * n
{
  var exc: Ref
  var return: Int
  var res1: Int
  var res2: Int
  exc := null
  // REWRITTEN
  // res1 := incr(this, tid, n)
  // Save old values
  var oldThisCount: Int := this.count
  // Exhale preconditions
  exhale this != null && acc(this.count, 1 * write)
  // Inhale postconditions
  inhale acc(this.count, 1 * write / 2) && this.count == oldThisCount + n
  // REWRITTEN
  // res2 := incr(this, tid, n)
  // Save old values
  oldThisCount := this.count
  // Exhale preconditions
  exhale this != null && acc(this.count, 1 * write)
  // Inhale postconditions
  inhale acc(this.count, 1 * write / 2) && this.count == oldThisCount + n
  res := return
  assert exc == null
}

